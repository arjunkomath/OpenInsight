{
	"name": "openinsight",
	"version": "0.1.0",
	"description": "Query your database with natural language",
	"license": "MIT",
	"author": "Arjun Komath",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/arjunkomath/openinsight.git"
	},
	"homepage": "https://github.com/arjunkomath/openinsight#readme",
	"bugs": {
		"url": "https://github.com/arjunkomath/openinsight/issues"
	},
	"keywords": [
		"database",
		"sql",
		"ai",
		"cli",
		"terminal",
		"query",
		"postgresql",
		"mysql",
		"sqlite",
		"natural-language",
		"openrouter",
		"gemini"
	],
	"bin": "dist/cli.js",
	"type": "module",
	"engines": {
		"node": ">=16"
	},
	"scripts": {
		"build": "babel --out-dir=dist source",
		"dev": "babel --out-dir=dist --watch source",
		"test": "prettier --check . && xo && ava"
	},
	"files": [
		"dist"
	],
	"dependencies": {
		"@inkjs/ui": "^2.0.0",
		"better-sqlite3": "^11.7.0",
		"ink": "^4.1.0",
		"ink-select-input": "^5.0.0",
		"ink-text-input": "^5.0.0",
		"meow": "^11.0.0",
		"mysql2": "^3.11.5",
		"pg": "^8.13.1",
		"react": "^18.2.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.21.0",
		"@babel/preset-react": "^7.18.6",
		"@vdemedes/prettier-config": "^2.0.1",
		"ava": "^5.2.0",
		"chalk": "^5.2.0",
		"eslint-config-xo-react": "^0.27.0",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"import-jsx": "^5.0.0",
		"ink-testing-library": "^3.0.0",
		"prettier": "^2.8.7",
		"xo": "^0.53.1"
	},
	"ava": {
		"environmentVariables": {
			"NODE_NO_WARNINGS": "1"
		},
		"nodeArguments": [
			"--loader=import-jsx"
		]
	},
	"xo": {
		"extends": "xo-react",
		"prettier": true,
		"rules": {
			"react/prop-types": "off",
			"unicorn/expiring-todo-comments": "off"
		}
	},
	"prettier": "@vdemedes/prettier-config",
	"babel": {
		"presets": [
			"@babel/preset-react"
		]
	}
}
